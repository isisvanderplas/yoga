    #block
      h1#title(style='text-align:center;')
        | Send me a message
      form.contactform(action='#', method='post')
        div
          input.field(type='text', name='user_name', placeholder=' Your name')
        div
          input.field(type='email', name='user_email', placeholder=' Your email')
